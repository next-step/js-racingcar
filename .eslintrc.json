{
  "rules": {
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "airbnb/base"],
  "ignorePatterns": ["webpack.config.js", "loader.js", "/__tests__"]
}
