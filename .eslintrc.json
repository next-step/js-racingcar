{
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "prettier/prettier": ["off"],
    "lines-between-class-members": "off",
    "class-methods-use-this": "off"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "airbnb/base", "prettier"],
  "ignorePatterns": ["webpack.config.js", "loader.js", "/__tests__"]
}
