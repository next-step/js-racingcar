{
	"env": {
		"browser": true,
		"es2021": true,
		"cypress/globals": true
	},
	"plugins": [
		"cypress",
		"prettier"
	],
	"extends": [
		"prettier",
		"airbnb-base",
		"plugin:cypress/recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "error",
		"import/extensions": "off"
	}
}
