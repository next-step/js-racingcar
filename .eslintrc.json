{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended", "plugin:cypress/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "max-depth": ["error", 2],
    "no-alert":"off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
