{
  "rules": {
    "indent": ["error", 2],
    "eqeqeq": 2,
    "no-var": "error",
    "no-else-return":"error"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ]
}
